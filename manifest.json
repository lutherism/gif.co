{
  "manifest_version": 2,

  "name": "Gif.co",
  "description": "The easiest way to express yourself with gifs.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "selected.html"
  },
  "permissions": [
    "https://api.imgur.com/",
    "http://i.imgur.com/"
  ]
}
